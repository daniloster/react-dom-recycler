{
  "name": "react-dom-recycler",
  "version": "1.0.0",
  "description": "npm module for recycling DOM element in react",
  "main": "lib/index.js",
  "browser" : {
    "fs":      false
  },
  "scripts": {
    "test": "NODE_ENV=test karma start karma.config.js --single-run --no-auto-watch",
    "clean": "rm -rf lib && mkdir lib",
    "precompile": "npm run clean",
    "compile": "babel src --out-dir lib --copy-files --source-maps --presets=es2015,react,stage-0 --plugins=transform-es2015-modules-umd",
    "build": "NODE_ENV=production webpack --config webpack.config.js",
    "start": "NODE_ENV=dev webpack-dev-server --content-base dist --hot --inline"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daniloster/react-list-recycler.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "integration",
    "integrating",
    "javascript",
    "js",
    "dom",
    "recycler"
  ],
  "author": "Danilo Castro",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/daniloster/react-list-recycler/issues"
  },
  "homepage": "https://github.com/daniloster/react-list-recycler#readme",
  "devDependencies": {
    "babel-core": "6.14.0",
    "babel-polyfill": "6.13.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-0": "6.5.0",
    "babel-preset-airbnb": "2.0.0",
    "eslint": "3.7.1",
    "eslint-config-airbnb": "11.0.0",
    "eslint-loader": "1.5.0",
    "eslint-plugin-import": "1.14.0",
    "eslint-plugin-jsx-a11y": "2.2.1",
    "eslint-plugin-react": "6.2.0",
    "eslint-plugin-jasmine": "1.8.1",
    "file-loader": "0.9.0",
    "img-loader": "1.3.1",
    "node-sass": "3.9.3",
    "autoprefixer": "6.4.1",
    "css-loader": "0.25.0",
    "postcss": "5.2.0",
    "postcss-loader": "0.13.0",
    "react-hot-loader": "3.0.0-beta.3",
    "sass-loader": "4.0.2",
